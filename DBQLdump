/****** Object:  Trigger [dbo].[LOAN_STATUS_LOG_trigger]    Script Date: 12/2/2015 20:49:41 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE TRIGGER [dbo].[LOAN_STATUS_LOG_trigger] ON [dbo].[LOAN] AFTER INSERT
AS

declare @loan_id varchar(50);
declare @loan_status varchar(50);


select @loan_id = LOAN_ID from INSERTED;
select @loan_status = LOAN_STATUS from inserted;

insert into LOAN_STATUS_LOG (LOAN_ID,LOAN_STATUS,TIMESTAMP)
values(@loan_id,@loan_status,CURRENT_TIMESTAMP)


GO
/****** Object:  Trigger [dbo].[LOAN_STATUS_LOG_trigger2]    Script Date: 12/2/2015 20:49:41 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE TRIGGER [dbo].[LOAN_STATUS_LOG_trigger2] ON [dbo].[LOAN] AFTER update
AS

declare @id varchar(50);
declare @status varchar(50);


select @id = LOAN_ID from deleted;
select @status = LOAN_STATUS from inserted;

insert into LOAN_STATUS_LOG (LOAN_ID,LOAN_STATUS,TIMESTAMP)
values(@id,@status,CURRENT_TIMESTAMP)
GO
USE [master]
GO
ALTER DATABASE [MSIS-2603-Project<DBQL>] SET  READ_WRITE 
GO
